{
  "name": "ngx-md-parse-env",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "npm run release:clean && npm run release:ngc && npm run release:rollup:es && npm run release:rollup:umd && npm run release:copy:pkg && npm run release:copy:readme && npm run release:publish",
    "release:clean": "rimraf ./dist",
    "release:ngc": "ngc -p tsconfig-release.json",
    "release:rollup:es": "rollup -c rollup.es.config.js",
    "release:rollup:umd": "rollup -c rollup.umd.config.js",
    "release:copy:pkg": "ncp ./src/package/package.json ./dist/package.json",
    "release:copy:readme": "ncp ./README.md ./dist/README.md",
    "release:publish": "npm publish ./dist --access=public"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "he": "^1.1.1",
    "highlight.js": "^9.10.0",
    "rxjs": "^5.1.0",
    "showdown": "^1.6.4",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ncp": "^2.0.0",
    "protractor": "~5.1.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  }
}
